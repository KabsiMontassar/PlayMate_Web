<!DOCTYPE html>

<title>Participation</title>

{% block body %}
    <h1>Participation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ participation.id }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ participation.status ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Datec</th>
                <td>{{ participation.datec ? participation.datec|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Nomequipe</th>
                <td>{{ participation.nomequipe }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_participation_index') }}">back to list</a>

   

    {{ include('Back/GestionEvenement/participation/_delete_form.html.twig') }}
{% endblock %}
