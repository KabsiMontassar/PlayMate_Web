<!DOCTYPE html>

<title>Reservation</title>

{% block body %}
<h1>Reservation</h1>

<table class="table">
    <tbody>
        <tr>
            <th>Idreservation</th>
            <td>{{ reservation.idreservation }}</td>
        </tr>
        <tr>
            <th>Isconfirm</th>
            <td>{{ reservation.isconfirm ? 'Yes' : 'No' }}</td>
        </tr>
        <tr>
            <th>Datereservation</th>
            <td>{{ reservation.datereservation }}</td>
        </tr>
        <tr>
            <th>Heurereservation</th>
            <td>{{ reservation.heurereservation }}</td>
        </tr>
        <tr>
            <th>Type</th>
            <td>{{ reservation.type }}</td>
        </tr>
    </tbody>
</table>

<a href="{{ path('app_reservation_index') }}">back to list</a>

<a href="{{ path('app_reservation_edit', {'idreservation': reservation.idreservation}) }}">edit</a>

{{ include('Back/GestionReservation/reservation/_delete_form.html.twig') }}
{% endblock %}